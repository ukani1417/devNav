{
  "name": "devnav",
  "version": "1.0.0",
  "description": "Fast URL navigation for developers",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src"
  },
  "keywords": [
    "chrome-extension",
    "url-shortcuts",
    "developer-tools",
    "navigation"
  ],
  "author": "DevNav Team",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/chrome": "^0.0.270",
    "@types/jest": "^29.0.0",
    "@types/jsdom": "^27.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.3.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.4.0",
    "typescript": "^5.0.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  }
}
